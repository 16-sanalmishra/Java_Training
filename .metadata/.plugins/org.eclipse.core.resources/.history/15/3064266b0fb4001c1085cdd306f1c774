package com.JavaLive.Session5;

public class letsGetData implements Comparable<letsGetData > {

	private int age;
	private String name;
	
	public letsGetData(int age, String name) {
		
		this.age = age;
		this.name = name;
	}
	
	




	public int getAge() {
		return age;
	}






	public void setAge(int age) {
		this.age = age;
	}






	public String getName() {
		return name;
	}






	public void setName(String name) {
		this.name = name;
	}






	public letsGetData() {
		// TODO Auto-generated constructor stub
	}



	@Override
	public String toString() {
		return "letsGetData [age=" + age + ", name=" + name + "]";
	}



	@Override
	public int compareTo(letsGetData o) {
		// TODO Auto-generated method stub
		return ((Integer)(this.age)).compareTo(o.age);
	}
}
