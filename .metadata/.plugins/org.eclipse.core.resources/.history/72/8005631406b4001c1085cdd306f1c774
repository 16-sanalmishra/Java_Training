package com.JavaLive.Session5;

import java.util.*;



public class PracticeExamples {
	public static void main(String[] args) {
		letsGetData d1 = new letsGetData(1, "Sanal");
		letsGetData d2 = new letsGetData(22, "Aryan");
		letsGetData d3 = new letsGetData(10, "Sonal");
		letsGetData d4 = new letsGetData(0, "Sakshi");
		letsGetData d5 = new letsGetData(91, "Sohak");
		letsGetData d6 = new letsGetData(100, "Snehil");
		
		Set<letsGetData> si = new TreeSet<>();
		si.add(d1);
		si.add(d2);
		si.add(d3);
		si.add(d4);
		si.add(d5);
		si.add(d6);
		
	System.out.println(si.toString());
	}
}

	
class letsGetData implements Comparable<letsGetData>{

		private int age;
		private String name;
		public letsGetData(int age, String name) {
			
			this.age = age;
			this.name = name;
		}
		@Override
		public int compareTo(letsGetData o) {
			// TODO Auto-generated method stub
			return ((Integer)this.age).compareTo(o.age);
		}
		
	}
	

	
	

