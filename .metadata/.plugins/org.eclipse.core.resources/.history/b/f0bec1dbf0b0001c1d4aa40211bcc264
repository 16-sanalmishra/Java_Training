package com.JavaLive.Session1;

public class TypeCasting {
	public void typeCast() {
		char s2 = 'A';
		int n = (int) s2;
		System.out.println(n);
		
		long  l = n;
		System.out.println(l);
		
		char c = (char) n;
		System.out.println(c);
		
		String s = "Sanal";
		int s1 = Integer.parseInt(s);
		System.out.println(s1);
	}
}
