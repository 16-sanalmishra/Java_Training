package com.JavaLive.Session1;

import java.util.*;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		TypeCasting tc = new TypeCasting();
		tc.typeCast();
		System.out.println(tc instanceof TypeCasting);
		
		System.out.println("-----------------------");
		int n = sc.nextInt();
		int [] a = new int [n];
		
		for(int i = 0; i<n; i++) {
			a[i] = sc.nextInt();
		}
		
		System.out.println("The Entered array is...........");
		for(int arr : a ) {
			System.out.print(arr);
			
		}
		
	}

}
