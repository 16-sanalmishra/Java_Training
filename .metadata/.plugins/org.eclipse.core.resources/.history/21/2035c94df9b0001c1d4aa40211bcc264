package com.JavaLive.Session2;

public class PracticeConstructor {
	private String name;
	private int age;
	private float height;
	
	public PracticeConstructor(String name, int age, float height) {
		
		this.name = name;
		this.age = age;
		this.height = height;
	}
	
	public PracticeConstructor() {
		super();
	}
	
	public String getName() {
		return name;
	}
	


	public void setName(String name) {
		this.name = name;
	}
	@Override
	public String toString() {
		return "PracticeConstructor [name=" + name + ", age=" + age + ", height=" + height + "]";
	}

	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public float getHeight() {
		return height;
	}
	public void setHeight(float height) {
		this.height = height;
	}
	
	
	
	
}
